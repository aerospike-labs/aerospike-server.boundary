{
  "description": "Aerospike Server",
  "command": "node . --interval $(pollInterval)",
  "postExtract": "npm install",
  "ignore": "node_modules",
  "metrics": [
    "AEROSPIKE_READS",
    "AEROSPIKE_READS_OK",
    "AEROSPIKE_READS_ERR",
    "AEROSPIKE_WRITES",
    "AEROSPIKE_WRITES_OK",
    "AEROSPIKE_WRITES_ERR",
    "AEROSPIKE_QUERIES",
    "AEROSPIKE_QUERIES_OK",
    "AEROSPIKE_QUERIES_ERR",
    "AEROSPIKE_AGGS",
    "AEROSPIKE_AGGS_OK",
    "AEROSPIKE_AGGS_ERR",
    "AEROSPIKE_PROXIES",
    "AEROSPIKE_PROXIES_OK",
    "AEROSPIKE_PROXIES_ERR",
    "AEROSPIKE_MEM_TOTAL",
    "AEROSPIKE_MEM_USED",
    "AEROSPIKE_MEM_USED_DATA",
    "AEROSPIKE_MEM_USED_INDEX",
    "AEROSPIKE_MEM_USED_SINDEX",
    "AEROSPIKE_OBJECTS",
    "AEROSPIKE_OBJECTS_EXPIRED",
    "AEROSPIKE_OBJECTS_EVICTED",
    "AEROSPIKE_CLIENTS",
    "AEROSPIKE_TXNS",
    "AEROSPIKE_TXNS_WAITING",
    "AEROSPIKE_READ_1MS",
    "AEROSPIKE_READS_8MS",
    "AEROSPIKE_READS_64MS",
    "AEROSPIKE_WRITE_1MS",
    "AEROSPIKE_WRITE_8MS",
    "AEROSPIKE_WRITE_64MS"
  ]
}
